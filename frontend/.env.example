# Frontend Configuration
# Copy this file to .env and modify as needed
# Note: Docker-specific URLs are set in docker-compose.yml

# Backend URL (client-side)
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000

# MinIO URL (client-side)
NEXT_PUBLIC_MINIO_BASE_URL=http://localhost:9000

# MinIO URL replacement for Docker
PUBLIC_MINIO_URL_SET=true

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-5-nano
OPENAI_TEMPERATURE=1
