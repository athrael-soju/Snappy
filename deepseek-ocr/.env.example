# DeepSeek-OCR Service Environment Variables

# API Security
SERVICE_API_KEY=dev-key-change-in-production
REQUIRE_API_KEY=false

# Public URL Configuration
PUBLIC_HOST=localhost
PUBLIC_PORT=7860

# DeepSeek Model Configuration
DEEPSEEK_MODEL_REVISION=2c968b433af61a059311cbf8997765023806a24d
DEEPSEEK_BASE_SIZE=1024
DEEPSEEK_IMAGE_SIZE=640
DEEPSEEK_CROP_MODE=true

# GPU Configuration (for GPU profile)
DEEPSEEK_GPU_COUNT=all

# Rate Limiting
RATE_LIMIT_REQUESTS=30
RATE_LIMIT_WINDOW_SECONDS=60
MAX_UPLOAD_BYTES=5242880
